{
  "name": "blade",
  "version": "1.0.0",
  "description": "旨在打造自己顺手的工具库，并且以项目驱动学习",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:electron\"",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "dev:server": "cross-env NODE_ENV=development webpack-dev-server --config  ./build/webpack.dev.js",
    "build": "concurrently \"npm run build:server\" \"npm run build:electron\"",
    "build:server": "cross-env NODE_ENV=production webpack  --config ./build/webpack.prod.js",
    "build:electron": "cross-env NODE_ENV=production electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Liugq5713/blade.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Liugq5713/blade/issues"
  },
  "homepage": "https://github.com/Liugq5713/blade#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.6.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "antd": "^3.7.3",
    "font-awesome": "^4.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "resize-img": "^1.1.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "stage-2",
      "react"
    ],
    "plugins": [
      [
        "transform-runtime"
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  }
}
